<div class="container">
  <div class="form-wrapper" style="width: 400px; margin: 0 auto; padding:60px;">
    <h2>Edit your renting</h2>

    <%= simple_form_for [@superpower, @renting] do |f| %>
      <%= f.input :start_date, as: :string, required: false, input_html: { id: "range_start" } %>
      <%= f.input :end_date, as: :string, required: false, input_html: { id: "range_end" } %>
      <%= f.button :submit, "Go Superpower", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
